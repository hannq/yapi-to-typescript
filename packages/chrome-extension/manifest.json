{
  "name": "Yapi to TypeScript",
  "description": "a Yapi to typescript tool.",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "host_permissions": ["*://*\/*"],
  "action": {
    "default_title": "Yapi to TypeScript",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
 "background": {
    "service_worker": "background.js"
  }
}
